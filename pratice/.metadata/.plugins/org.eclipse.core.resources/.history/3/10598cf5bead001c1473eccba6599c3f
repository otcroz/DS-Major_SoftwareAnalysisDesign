package study.ch04.pratice;

import study.ch04.cookie.Cookie;
import study.ch04.cookie.CookieStore;
import study.ch04.framework.Product;
import study.ch04.framework.Store;

public class Main {

	public static void main(String[] args) {
		Store store_Cookie = new CookieStore();
		
		//Store의 create를 통해 쿠키를 만듦(만들고 판매)
		Product cookie1 = store_Cookie.create("초콜릿 쿠키");
		Product cookie2 = store_Cookie.create("버터 쿠키");
		
		//쿠키를 판매함
		cookie1.sale();
		cookie2.sale();
		
		//쿠키 이름 목록 출력
		Iterator it = store_Cookie.iterator();
	}

}
