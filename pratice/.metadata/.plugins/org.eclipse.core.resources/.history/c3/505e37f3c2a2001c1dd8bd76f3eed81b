package study.ch01;

public class LearningTogetherIterator {
	Student students[];
	int last;
	
	public LearningTogetherIterator(int maxsize) {
		students = new Student[maxsize];
		last = 0;
	}
	
	public Student getStudentFrom(int index) {
		return students[index];
	}
	
	public void appendStudent(Student s) {
		students[last] = s;
		last++;
	}
	
	public int getLength() {
		
	}
}
