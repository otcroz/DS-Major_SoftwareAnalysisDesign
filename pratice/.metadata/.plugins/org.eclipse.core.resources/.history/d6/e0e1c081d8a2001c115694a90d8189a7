package BookShelf;

public class BookShelf {
	Book[] books;
	int last;
	
	public BookShelf(int maxsize) {
		books = new Book[maxsize];
		last = 0;
	}
	
	public void appendBook(Book book){
		books[last] = book;
		last++;
	}
	
	public int getLength() {
		return last;
	}
	
	public Book getBookFrom(int index) {
		return books[index];
	}
}
