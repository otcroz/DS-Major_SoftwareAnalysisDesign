package ch01.BookShelf;

public class BookShelfIteratorBackword implements Iterator {
	BookShelf bookshelf;
	int index;
	
	BookShelfIteratorBackword(BookShelf bookshelf){
		this.bookshelf = bookshelf;
		this.index = bookshelf.getLength();
	}
	
	@Override
	public boolean hasNext() {
		if (index < bookshelf.getLength()) return true;
		else return false;
	}

	@Override
	public Object next() {
		bookshelf.getBookFrom(bookshelf.getLength());
		
		return null;
	}

}
